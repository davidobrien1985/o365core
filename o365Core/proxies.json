{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "slackproxy": {
      "matchCondition": {
        "methods": [ "POST" ],
        "route": "/api/o365bot"
      },
      "backendUri": "https://%WEBSITE_SITE_NAME%.azurewebsites.net/api/receiveJobFromSlack?code=%receiveSlackFunctionKey%",
      "debug": true
    }
  }
}